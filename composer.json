{
  "name": "4myth/laravel-tools",
  "description": "Tools of laravel framework",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "laravel/framework": ">=9.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.13",
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.1",
    "spatie/laravel-ignition": "^2.0",
    "simplesoftwareio/simple-qrcode": "~4",
    "maatwebsite/excel": "^3.1",
    "ctwillie/expo-server-sdk-php": "^2.0",
    "geniusts/hijri-dates": ">=1.0",
    "akaunting/laravel-setting": ">=1.0",
    "bavix/laravel-wallet": "^9.6",
    "beyondcode/laravel-websockets": "^1.13",
    "barryvdh/laravel-snappy": "^1.0@dev",
    "spatie/laravel-medialibrary": "^11.0.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Myth\\LaravelTools\\": "src/"
    },
    "files": [
      "src/Utilities/Functions.php"
    ]
  },
  "authors": [
    {
      "name": "MyTh",
      "email": "mythpe@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Myth\\LaravelTools\\Providers\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
