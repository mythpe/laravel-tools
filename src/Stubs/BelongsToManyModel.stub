namespace App\Traits\BelongsToMany;

use App\Models\{model} as Model;
use Illuminate\Database\Eloquent\Relations\BelongsToMany;

trait BelongsToMany{modelName}
{

    /**
     * @return BelongsToMany
     */
    public function {modelPluralName}(): BelongsToMany
    {
        return $this->belongsToMany(Model::class)->withTimestamps();
    }
}
